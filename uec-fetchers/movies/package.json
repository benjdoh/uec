{
	"name": "movies",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"scripts": {
		"build": "pnpm run build:movies && pnpm run build:showings",
		"build:movies": "esbuild movies.js --bundle --platform=node --outfile=dist/movies/index.js",
		"build:showings": "esbuild showings.js --bundle --platform=node --outfile=dist/showings/index.js",
		"build:movies:ru": "rollup -o=movies.bundled.js -f=es -p=@rollup/plugin-json movies.js",
		"build:showings:ru": "rollup -o=showings.bundled.js -f=es -p=@rollup/plugin-json showings.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@planetscale/database": "^1.11.0",
		"@rollup/plugin-json": "^6.0.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.6",
		"esbuild": "^0.19.3",
		"puppeteer": "^21.3.4",
		"rollup": "^3.29.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
